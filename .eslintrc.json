{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "indent": "off",
        "no-unused-vars": "off",
        "arrow-body-style": "off",
        "import/no-unresolved": "error",
        "import/no-extraneous-dependencies": "off",
        "import/no-duplicates": "error"
    }
}
